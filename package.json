  "bugs": {
    "url": "https://github.com/rune-b/xwing-cards/issues"
  },
  "homepage": "https://github.com/rune-b/xwing-cards#readme",
  "devDependencies": {
    "elm": "^0.17.1",
    "elm-live": "^2.6.0"
  }
}

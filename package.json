    "build": "elm-make ./src/app.elm --output=dist/elm.js",
    "start": "elm-live ./src/app.elm --output=dist/elm.js --open",
    "update": "git submodule update --init --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rune-b/xwing-cards.git"
  },
